<h1>Users#show</h1>
<p>Hello, <%=@user.name%>!</p>
<%@user.trips.each do |t|%>
<p>Trip: <%=t.id%></p>
<%end%>


<% @trips.each do |t| %>
  <% t.pickups.each do |x| %>
    <p>Pick ups pending: <%=x.id%></p>
  <%end%>
<%end%>

<% @user.recycling_pickups.each do |t| %>
  <p>Pick ups pending: <%=t.id%></p>
<%end%>

<p>Credit count: <%= @user.credit_count%></p>
